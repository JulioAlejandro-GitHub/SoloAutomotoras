/*---------------resets----------------------*/
body,ul,li,ol,h1,h2,h3,h4,h5,h6,pre,dl,dt,dd,fieldset,p,blockquote,th,td,input[type="checkbox"],input[type="radio"]{
	margin: 0;
	padding: 0;	
}
img{
	border: 0 none;
}
img{
	border: 0 none;
}
ul,li,ol{
	list-style-type: none;
}
table {
	border-collapse:collapse;
	border-spacing:0;
}
h1,h2,h3,h4,h5,h6 {
	font-size: 100%;
	font-weight: normal;
	font-style: normal;
}

/*------gral------*/
body{
	background:#fff;
}

html,body,div,p {
	font-family: 'Lucida Grande', Tahoma, Verdana, Helvetica, Arial, Geneva, sans-serif;
	font-weight: normal;
	margin:0;
	padding:0;
}

a{
	text-decoration:none;
}
a:link {
	color: #016fbc;
}
a:visited {
	color: #016fbc;
}
a:hover {
	text-decoration: underline;
}
a:active {
	color: #333;
	text-decoration: underline;
}
/*Buttons*/
input[type="submit"], input[type="button"], button,.btn,a.btn {
	background: #ffab19;
	color:#000;
	cursor:pointer;
    text-align:right;
	font-weight: bold;
	font-size:14px;
	padding: 8px 15px;
	-moz-border-radius:4px;
	border-radius: 4px;
	-webkit-border-radius:4px;
	border: 1px solid #bd8021;
	-moz-box-shadow:0 1px 0 rgba(255, 255, 255, 0.4) inset, 0 1px 1px rgba(0, 0, 0, 0.2);;
   	-webkit-box-shadow: 0 1px 0 rgba(255, 255, 255, 0.4) inset, 0 1px 1px rgba(0, 0, 0, 0.2);;
   	box-shadow:0 1px 0 rgba(255, 255, 255, 0.4) inset, 0 1px 1px rgba(0, 0, 0, 0.2);;
   	text-shadow: #ffdf7e 0 1px 0;
	margin-left: 2px;
	margin-right: 2px;
}
input[type="submit"]:hover, input[type="button"]:hover, button:hover,.btn:hover{
	background: #fbc762;
	text-decoration: none;
}
input[type="submit"]:active, input[type="button"]:active, button:active,.btn:active{
	background: #fbc762;
	text-decoration: none;
	-webkit-box-shadow: inset 0 1px 4px rgba(0, 0, 0, 0.6);
    -moz-box-shadow: inset 0 1px 4px rgba(0, 0, 0, 0.6);
    box-shadow: inset 0 1px 4px rgba(0, 0, 0, 0.6);
}

input[type="submit"].disabled, input[type="button"].disabled, button.disabled,.btn.disabled{
	background:#ccc;
	box-shadow:none;
	-moz-box-shadow:none;
   	-webkit-box-shadow:none;
	border-color: transparent;
	cursor: default;
}
input[type="text"],input[type="password"],input[type="number"],textarea{
	-moz-border-radius:3px;
	border-radius: 3px;
	-webkit-border-radius:3px;
	border: 1px solid #bbb;
	font-size: 12px;
	padding: 4px;
	color: #333;
	-moz-box-shadow:inset 0px 1px 3px #ccc;
   	-webkit-box-shadow:inset 0px 1px 3px #ccc;
   	box-shadow:inset 0px 1px 3px #ccc;
   	margin-bottom: 10px;
}
input[type="text"]:focus,input[type="password"]:focus,input[type="number"]:focus,textarea:focus{
	-moz-box-shadow:inset 0px 1px 3px #bababa;
   	-webkit-box-shadow:inset 0px 1px 3px #bababa;
   	box-shadow:inset 0px 1px 3px #bababa;
   	border: 1px solid #fa9e11;
   	outline: 0;
}
input[type="text"]:disabled,input[type="password"]:disabled,input[type="number"]:disabled,textarea:disabled{
	moz-box-shadow:none;
   	-webkit-box-shadow:none;
   	box-shadow:none;
	border: 1px solid #ddd;
	background: #fcfcfc;
	color: #bbb;
}

